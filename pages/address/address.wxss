.Distance{/*添加商品弹框*/
  width: 100%;
  height:1206rpx;
}
.DistanceContent{
  width: 100%;
  height: 100%;
}
.contentName{
  width: 100%;
  height: 88rpx;
  border: 1px solid rgba(238,238,238,1);
  text-align: center;
  line-height: 88rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.scrollDistance{
  width: 100%;
  height: 100%;
}
.adress{
  width:720rpx;
  height:151rpx;
  margin: 0 auto;
  border: 1px solid  rgba(238,238,238,1);
  display: flex;
  align-items: center;
  justify-content: space-between
}
.adressName{
  width: 618rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
}
.text {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.txt {
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(153,153,153,1);
}
.operation{
  width: 102rpx;
  height: 26rpx;
  border-left:1rpx solid rgba(224,224,224,1);
  font-size:26rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  text-align: center;
  color:rgba(178,178,178,1);
  line-height: 26rpx;
}
.upDistance{
  width: 100%;
  height:100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.upDistance>image{
  width: 38rpx;
  height: 38rpx;
}
.upDistance>image{
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(153,153,153,1);
  padding-left: 20rpx;
}