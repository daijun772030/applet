.setAplet{
  width: 100%;
  height:1206rpx;
  background-color:rgba(245,245,245,1);
}
.address{
  width: 100%;
  height: 103rpx;
  margin-top: 18rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
  background-color: white;
}
.address>text {
  margin-left: 30rpx;
}
.address>image {
  width: 11rpx;
  height: 22rpx;
  margin-right: 30rpx;
}
.deletLogin{
  display: flex;
  align-items: center;
  margin-top: 30rpx;
}
.deletLogin>button {
  width: 690rpx;
  height: 88rpx;
  border: none;
  outline: none;
  border-radius: 5rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  background:rgba(46,227,157,1);
  color: white;
  line-height: 88rpx;
  text-align: center;
}
button::after{
  border:none;
  border-radius: 0;
}