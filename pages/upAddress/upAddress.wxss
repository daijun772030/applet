.upAddress{
  width: 700rpx;
  height: 100%;
  margin: 0 auto;
}
.address,.housNumber,.contact,.sex,.phone{
  width: 100%;
  height: 103rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.address>text,.housNumber>text,.contact>text,.sex>text,.phone>text{
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(178,178,178,1);
}
.addContent,.housName,.contactName,.sexName,.phoneName{
  width: 562rpx;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.addContent>text {
  width: 548rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
}
.addContent>image {
  width: 11rpx;
  height: 22rpx;
}
.housName>input,.contactName>input,.phoneName>input {
  width: 100%;
  height: 100%;
  font-size:30rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  /* color:rgba(178,178,178,1); */
}
button {
  width: 690rpx;
  height: 88rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  text-align: center;
  line-height: 88rpx;
  background:rgba(0,212,160,1);
  border-radius:2px;
  outline: none;
  border: none;
}
.radio-group{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}
.radio {
  padding-right: 49rpx;
}
radio .wx-radio-input{
  height: 32rpx;
  width: 32rpx;
}