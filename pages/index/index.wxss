.sjkjWechat{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow-x: hidden;
}
.shopCarImg{
  width:68rpx;
  height:68rpx;
  background:rgba(255,255,255,1);
  border:1px solid #e0e0e0;
  border-radius:50%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 32rpx;
  right: 21rpx;
  z-index: 99999;
}
.shopCarImg>image {
  width: 100%;
  height: 100%;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.sjkjIndex{
width:100%;
height:130rpx;
background:rgba(0,212,160,1);
display: flex;
flex-direction: column;
justify-content: space-around;
align-items:center;
}
.sjNav {
  width: 710rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: flex-start;


}
.sjNav text{
font-size:32rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(255,255,255,1);
}
.sjNav input {
  width: 100%;
  height: 58rpx;
  background:rgba(255,255,255,1);
  border-radius:2px 2px 2px 2px;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(210,210,210,1);
}
/* 轮播样式 */
swiper {
  width: 710rpx;
  height: 270rpx;
  margin-top:20rpx;
}
swiper-item {
  width: 100%;
  height: 100%;
}
.slide-image{
  min-width: 100%;
  height: 100%;
}

/* 公告样式 */

.notice{
  width: 100%;
  height: 85rpx;
  margin-top: 20rpx;
  background:rgba(244,248,253,1);
  display:flex;
  justify-content: flex-start;
  align-items: center;
}
.noticeIcon{
  width: 60rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
}
.noticeIcon>image {
  height:60rpx;
  width: 68rpx;
}
.swiper {
  width: calc(100% - 60rpx);
  height: 100%;
}
.swperChid{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}
.swperChid>text {
  font-size:20rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  padding-left: 11rpx;
  height: 100%;
  line-height: 85rpx;
}
/* 首页导航列表 */
.sjkjNavList{
  width: 100%;
  margin: 61rpx auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-items: center;
}
.ListTop,.listBootm{
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.ListTop{
  margin-bottom: 74rpx;
}
.navListItem{
  width:110rpx;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center
}
.navListItem text {
  font-size:22rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
}
.navListItem image {
  max-width:74rpx;
  max-height: 80rpx;
}
.vicinity{
  width: 99%;
  /* margin-left: 20rpx; */
  padding-left: 20rpx;
  margin-bottom: 40rpx;
  margin-top: 38rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
}
/* .vicinity text {
  padding-left: 20rpx;
} */
.vicinityList{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  align-items: center;
}
.vicinityListItem{
  width: 40%;
  margin-bottom: 64rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  font-size:22rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  background:rgba(255,255,255,1);
  border:1px solid rgba(255,174,51,1);
  border-radius:5rpx;
  position: relative;
}
.sift{
  width:58rpx;
  height:29rpx;
  background:rgba(255,183,51,1);
  border-radius:5px 0px 10px 0px;
  position: absolute;
  top: 0;
  left: 0;
  font-size:22rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:30rpx;
  text-align: center;
}
.vicinityListItem image{
  width: 50%;
  height: 100rpx;
  margin-bottom: 40rpx;
  
}
.screen{
  width: 99%;
  height: 45rpx;
  padding-left: 20rpx;
  border-bottom: 1px solid rgba(238,238,238,1);
}
.topnav{
    position: fixed;
    top: 85rpx;
    z-index:99;
}
.screenChid {
  width: 60%;
  height: 100%;
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
/*商家列表展示*/
.merchant{
  width:710rpx;
  margin: 0 auto;
}
.merchantList{
  width: 100%;
  height:200rpx;
  /* max-height:200rpx; */
  border-bottom: 1px solid rgba(238,238,238,1);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.merchantListImg{
  width: 171rpx;
  height: 128rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.take {
width:100%;
height:47rpx;
background:rgba(0,0,0,1);
opacity:0.6;
border-radius:5px;
position: absolute;
bottom: 0;
left: 0;
font-size:26rpx;
font-family:PingFang-SC-Regular;
font-weight:400;
color:rgba(255,255,255,1);
text-align: center;
line-height: 47rpx;
}
.merchantListImg image {
  max-width: 100%;
  max-height: 100%;
}
.merchantMesg{
  width: calc(100% - 191rpx);
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.merchatOrder{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.orderLeft{
  width: 280rpx;
  font-size:22rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.grade {
  width: 113rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.grade image{
  width: 22rpx;
  height: 22rpx;
  padding-right: 7rpx;
}
.distance{
  font-size:22rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.shopName{
  width: 100%;
font-size:26rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(102,102,102,1);
}
.mode {
  width: 100%;
  font-size:22rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.mode>.one{
  padding-right: 20rpx;
}
.discount{
  width: 100%;
  font-size:18rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(237,93,93,1);
}
.discountClass{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.discountClass text {
  border:1px solid rgba(255,196,196,1);
  border-radius:2rpx;
  margin-right: 16rpx;
}
.niClass{
  color:rgba(65,211,148,1);
  border:1px solid rgba(162,241,207,1);
  border-radius:2px;
}
.loadding{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  color: black;
}