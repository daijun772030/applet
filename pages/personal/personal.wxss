page {
  width: 100%;
  height: 100%;
}
.personal {
  width: 100%;
  height: 100%;
  background:rgba(247,247,247,1);
  overflow: hidden;
}
.navMsg{
  width:710rpx;
  height:167rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 0rpx 7rpx 0rpx rgba(83,238,160,0.38);
  border-radius:8rpx;
  margin:20rpx auto;
}
.navContent{
  width: 629rpx;
  height: 119rpx;
  margin-top:14rpx;
  display: flex;
  align-items: center;
}
.NavImg{
  width: 119rpx;
  height: 119rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 25rpx;
}
.NavImg>image {
  max-width: 108rpx;
  max-height: 108rpx;
  border-radius:50%;
}
.navRemark{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-right: 324rpx;
}
.navRemark>text {
  font-size:28rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(46,227,157,1);
}
.RemarkMsg{
  display: flex;
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(153,153,153,1);
  align-items: center;
}
.RemarkMsg>image {
  width:10rpx;
  height:12rpx;
  padding-left: 10rpx;
}
.service{
  display: flex;
  min-width: 46rpx;
  min-height: 40rpx;
  flex-direction:column;
  align-items: center;
  font-size:20rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.service>image {
  width: 46rpx;
  height: 40rpx;
}
.orderAll{
  width: 100%;
  height: 216rpx;
  background:rgba(255,255,255,1);
}
.orderName{
  width: 710rpx;
  height: 76rpx;
  border-bottom: 1rpx solid rgba(245,245,245,1);
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin:0 auto;
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(102,102,102,1);
}
.typeOrder {
  font-size:22rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.typeOrder>image {
  width: 12rpx;
  height: 22rpx;
  padding-left: 20rpx;
}
.orderList{
  width: 100%;
  height: calc( 100% - 76rpx );
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.listChid{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size:22rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.listChid>image {
  padding-bottom: 20rpx;
}
.dfk {
  width: 48rpx;
  height: 44rpx;
}
.djd {
  width: 40rpx;
  height: 45rpx;
}
.dsh {
  width: 48rpx ;
  height: 43rpx;
}
.dsh {
  width: 44rpx;
  height: 41rpx;
}
.ddp {
  width: 44rpx;
  height: 42rpx;
}
.listChidTwo{
  width: 398rpx;
}
.lazyRd{
  width: 38rpx;
  height: 46rpx;
}
.redMoney {
  width: 44rpx;
  height: 44rpx;
}
.setList{
  width: 100%;
  font-size:26rpx;
  margin-top: 20rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  background:rgba(255,255,255,1);
  color:rgba(102,102,102,1);

}
.setChid{
  width: 704rpx;
  height: 76rpx;
  display: flex;
  margin:0 auto;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid rgba(245,245,245,1);
}
.setChid>image {
  width: 11rpx;
  height: 22rpx;
}